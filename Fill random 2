Sub FillRandomlyUntilLastValue()
    Dim rngSourceA As Range
    Dim rngSourceC As Range
    Dim rngDest As Range
    Dim i As Integer
    Dim randIndex As Integer
    Dim lastRow As Long
    
    ' Find the last row of column A
    lastRow = Cells(Rows.Count, 1).End(xlUp).Row
    
    ' Set the range for the source columns
    Set rngSourceA = Range("A1:A" & lastRow)
    Set rngSourceC = Range("C1:C" & lastRow)
    
    ' Clear previous data in destination column
    Range("B:B").Clear
    
    ' Set the range for the destination column
    Set rngDest = Range("B1")
    
    ' Fill randomly in destination column until last value of source column A is reached
    For i = 1 To lastRow
        randIndex = Int((rngSourceA.Rows.Count * Rnd) + 1)
        If rngSourceA.Cells(randIndex, 1).Value <> "" Then
            rngDest.Value = rngSourceC.Cells(randIndex, 1).Value
            Set rngDest = rngDest.Offset(1, 0)
            rngSourceA.Cells(randIndex, 1).ClearContents ' Clear the used value from column A
        End If
    Next i
End Sub
